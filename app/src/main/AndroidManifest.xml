        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.fmway.userOperations.Profile"></activity>
        <activity android:name="com.fmway.userOperations.Payment" />
        <activity android:name="com.fmway.userOperations.HomePage" />
