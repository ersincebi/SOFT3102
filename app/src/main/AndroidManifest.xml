            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.fmway.tripOperations.AddTripActivity" />
        <activity android:name="com.fmway.tripOperations.ListTripActivity" />
        <activity android:name="com.fmway.tripOperations.driverFeedback" />
